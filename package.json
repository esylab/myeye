{
  "name": "myeye",
  "version": "1.0.0",
  "description": "A web app that assists blind individuals by providing audio descriptions of their surroundings and allowing them to control their audio playback.",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "client": "cd client && npm start",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Byansi Anthony, Daaki Benjamin",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.9",
    "nodemon": "^2.0.6"
    "@google/maps": "^4.3.0",
    "mapbox-sdk": "^2.1.2",
    "geojson": "^0.5.0"
  },
  "devDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  }
}

